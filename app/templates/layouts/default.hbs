	<head>
		<meta charset="utf-8"/>
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1"/>
    <title>{{title}}</title>

    <!-- build:css(.tmp) styles/css/main.min.css -->
    <link rel="stylesheet" href="./styles/css/main.css" />
    <!-- endbuild -->

		{{>head_meta}}
    {{>head}}
	</head>
	<body data-spy="scroll" data-target="#navbar" data-offcanvas-nav data-offcanvas-nav-xMax="{{settings.OFFCANVAS_NAV_X_MAX}}">

    {{> navbar_offcanvas}}

    <div class="flyout-cover offcanvas-slide"></div>

    <header class="navbar-top offcanvas-slide">

      {{> navbar}}

    </header>

    <div class="page scrollable offcanvas-slide">
      <!-- Upgrade browser. -->
      {{>upgrade_browser}}

			<!-- Page content -->
			{{>body}}

			<!-- Main docs footer -->
      {{> footer}}
		</div>


